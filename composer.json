{
    "name": "lammensj/resilient",
    "description": "Install Drupal with some defaults, based on personal experiences.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6",
        "drupal/advancedqueue": "^1.0@beta",
        "drupal/allowed_formats": "^1.2",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/adminimal_theme": "^1.5",
        "drupal/antibot": "^1.2",
        "drupal/big_pipe_sessionless": "^1.3",
        "drupal/block_field": "^1.0@alpha",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/bootstrap_library": "^1.11",
        "drupal/cache_control_override": "^1.0@alpha",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_readonly_menu_ui": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/crop": "^2.0",
        "drupal/csp": "^1.8",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/easy_email": "^1.0@beta",
        "drupal/eck": "^1.0@alpha",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity_class_formatter": "^1.2",
        "drupal/environment_indicator": "^4.0",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/field_group": "^3.0@RC",
        "drupal/fixed_text_link_formatter": "^1.5",
        "drupal/gdpr": "^3.0@beta",
        "drupal/google_tag": "^1.3",
        "drupal/honeypot": "^1.30",
        "drupal/image_effects": "^3.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/imce": "^1.7",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^5.0@beta",
        "drupal/login_destination": "^2.0@beta",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_multilingual": "^1.0@alpha",
        "drupal/metatag": "^1.11",
        "drupal/override_node_options": "^2.4",
        "drupal/paragraphs_admin": "^1.0",
        "drupal/paragraphs_ee": "^1.4",
        "drupal/paragraphs_features": "^1.7",
        "drupal/pathauto": "^1.6",
        "drupal/pathologic": "^1.0@alpha",
        "drupal/r4032login": "^2.0",
        "drupal/redirect": "^1.5",
        "drupal/responsive_favicons": "^1.5",
        "drupal/seckit": "^2.0",
        "drupal/schema_metatag": "^1.4",
        "drupal/simple_sitemap": "^3.5",
        "drupal/stage_file_proxy": "^1.0@RC",
        "drupal/svg_image": "^1.9",
        "drupal/swiftmailer_force_html": "^1.0@beta",
        "drupal/system_tags": "^1.6",
        "drupal/taxonomy_access_fix": "^3.3",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/webform": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "app/core": ["type:drupal-core"],
            "app/libraries/{$name}": ["type:drupal-library"],
            "app/modules/contrib/{$name}": ["type:drupal-module"],
            "app/profiles/contrib/{$name}": ["type:drupal-profile"],
            "app/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-paranoia": {
            "app-dir": "app",
            "web-dir": "web",
            "excludes": [
                "core/install.php",
                "sites/simpletest",
                "themes/custom/*/node_modules",
                "themes/custom/*/src"
            ]
        }
    }
}
