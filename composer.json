{
    "name": "lammensj/resilient",
    "description": "Install Drupal with some defaults, based on personal experiences.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.20",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/adminimal_theme": "^1.3",
        "drupal/block_field": "^1.0@alpha",
        "drupal/config_update": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/core": "~8.4",
        "drupal/field_group": "^1.0",
        "drupal/honeypot": "^1.27",
        "drupal/imce": "^1.6",
        "drupal/linkit": "^4.3",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/metatag": "^1.3",
        "drupal/override_node_options": "^2.3",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0@beta",
        "drupal/simple_sitemap": "^2.10",
        "drupal/swiftmailer_force_html": "^1.0@beta",
        "drupal/system_tags": "^1.1",
        "drupal/taxonomy_access_fix": "^2.1",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8|~3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
